{
  "name": "ucans",
  "version": "0.8.0",
  "description": "Typescript implementation of UCANs",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./dist/*": "./dist/*",
    "./*": "./dist/*",
    "./package.json": "./package.json"
  },
  "typings": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "dist/index.d.ts": [
        "dist/index.d.ts"
      ],
      "dist/*": [
        "dist/*"
      ],
      "*": [
        "dist/*"
      ]
    }
  },
  "author": "Daniel Holmgren <daniel@fission.codes>",
  "repository": {
    "type": "git",
    "url": "https://github.com/fission-suite/ucan"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "homepage": "https://guide.fission.codes",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=15"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "dev": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "one-webcrypto": "^1.0.1",
    "tweetnacl": "^1.0.3",
    "uint8arrays": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "jest": "^27.2.0",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  }
}
